{
  "name" : "ti-cms/yii2-app-ti-cms",
  "description" : "A TiCMS starter app for Yii2",
  "homepage" : "https://github.com/ti-cms/yii2-app-ti-cms",
  "version" : "0.0.2-alpha",
  "license" : "GPL-3.0",
  "type": "project",
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": ">=2.0.6",
    "ti-cms/ti-cms":"dev-sean-dev"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*"
  },
  "config": {
    "process-timeout": 1800
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
